<?xml version="1.0"?>
<extension key="biz.lcdservices.inheritedmembers" type="module">
  <file>inheritedmembers</file>
  <name>Rebuild Inherited Memberships</name>
  <description>Cycles through all parent memberships and updates the record to trigger inherited membership rebuilding. This is particular useful if you changed the membership type's inheritance relationships after existing memberships were created and now need the inherited records to be built.</description>
  <license>AGPL-3.0</license>
  <maintainer>
    <author>Lighthouse Consulting and Design</author>
    <email>dev@lcdservices.biz</email>
  </maintainer>
  <urls>
    <url desc="Main Extension Page">https://github.com/lcdservices/biz.lcdservices.inheritedmembers</url>
    <url desc="Documentation">https://github.com/lcdservices/biz.lcdservices.inheritedmembers</url>
    <url desc="Support">https://github.com/lcdservices/biz.lcdservices.inheritedmembers/issues</url>
    <url desc="Licensing">http://www.gnu.org/licenses/agpl-3.0.html</url>
  </urls>
  <releaseDate>2018-02-02</releaseDate>
  <version>1.0</version>
  <develStage>stable</develStage>
  <compatibility>
    <ver>4.7</ver>
  </compatibility>
  <comments></comments>
  <civix>
    <namespace>CRM/Inheritedmembers</namespace>
  </civix>
</extension>
